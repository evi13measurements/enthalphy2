CRIT7=`$GREP "Weighted mixed functional\:" $log | wc -l`
CRIT8=`$GREP "HF exchange\: * (0| )\.20000" $log | wc -l`
CRIT9=`$GREP "VWN\: * (0| )\.19000" $log | wc -l`
CRIT10=`$GREP "LYP\: * (0| )\.81000" $log | wc -l`
CRIT11=`$GREP "Becke\: * (0| )\.72000" $log | wc -l`
