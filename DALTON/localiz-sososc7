TEST[16]=`expr	$CRIT1`
CTRL[16]=1
ERROR[16]="? NOT CORRECT"

# ?
CRIT1=`$GREP "x * (0| )\.00000000 * (0| )\.00000000 * (0| )\.00000000" $log | wc -l`
CRIT2=`$GREP "y * (0| )\.00000000 * (0| )\.00000000 * (0| )\.00000000" $log | wc -l`
CRIT3=`$GREP "z * (\-0| \-)\.88289698 * \-2\.24410013 * \-7\.48551230" $log | wc -l`
TEST[17]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3`
CTRL[17]=3
ERROR[17]="? NOT CORRECT"

# ?
CRIT1=`$GREP "" $log | wc -l`
CRIT2=`$GREP "This is a DFT calculation of type\: B3LYP" $log | wc -l`
CRIT3=`$GREP "Weighted mixed functional\:" $log | wc -l`
CRIT4=`$GREP "HF exchange\: * (0| )\.20000" $log | wc -l`
CRIT5=`$GREP "VWN\: * (0| )\.19000" $log | wc -l`
CRIT6=`$GREP "LYP\: * (0| )\.81000" $log | wc -l`
CRIT7=`$GREP "Becke\: * (0| )\.72000" $log | wc -l`
CRIT8=`$GREP "Slater\: * (0| )\.80000" $log | wc -l`
