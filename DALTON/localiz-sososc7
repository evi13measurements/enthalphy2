#!/bin/sh
#
# This is the script for generating files for a specific Dalton test job.
#
# For the .check file ksh or bash is preferred, otherwise use sh
# (and hope it is not the old Bourne shell, which will not work)
#
if [ -x /bin/ksh ]; then
   CHECK_SHELL='#!/bin/ksh'
elif [ -x /bin/bash ]; then
   CHECK_SHELL='#!/bin/bash'
else
   CHECK_SHELL='#!/bin/sh'
fi


#######################################################################
#  TEST DESCRIPTION                                                   #
#######################################################################
cat > localiz-sososc7.info <<'%EOF%'
   localiz-sososc7
   ---------------
   Molecule:         ?
   Wave Function:    ?
   Test Purpose:     ?
%EOF%

#######################################################################
#  MOLECULE INPUT                                                     #
#######################################################################
cat > localiz-sososc7.mol <<'%EOF%'
BASIS
6-31G
NH3 : basis set 6-31G
Geometry : R_e=1.91315 a0, 106.67
    2    0            0.10D-15  0.10D-15
      7.00    1
N    0.0000000000000      0.0000000000000      0.1280609409808
      1.00    3
H1   1.5346152182086      0.8860105093353     -0.5931078547182
H2  -1.5346152182086      0.8860105093353     -0.5931078547182
H3   0.0000000000000     -1.7720210186705     -0.5931078547182
FINISH

%EOF%

#######################################################################
#  DALTON INPUT                                                       #
#######################################################################
cat > localiz-sososc7.dal <<'%EOF%'
**DALTON INPUT
.RUN PROPERTIES
*MOLBAS
.MAXPRI
 40
.PRINT
 2
**WAVE FUNCTION
.DFT
 B3LYP
*ORBITAL INPUT
.MOSTART
 H1DIAG
**PROPERTIES
.SPIN-SPIN
.LOCALIZATION
*TRPRSP
.MAXRED
 1800
*LINRES
.MAXRED
 1800
*SPIN-SPIN
.SOSOCC
.SOSOSC
  5  5000   5
  6    5   7   5.0D0
.ABUNDA
 0.04
#.PRINT
# 20
*LOCALIZATION
.FOSBOY
*END OF DALTON INPUT

%EOF%

#######################################################################
#  CHECK SCRIPT                                                       #
#######################################################################
echo $CHECK_SHELL > localiz-sososc7.check
cat >> localiz-sososc7.check <<'%EOF%'
log=$1

if [ `uname` = Linux ]; then
   GREP="egrep -a"
else
   GREP="egrep"
fi

# ?
CRIT1=`$GREP "Principal moments of inertia \(u\*A\*\*2\) and principal axes" $log | wc -l`
CRIT2=`$GREP "\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-" $log | wc -l`
TEST[1]=`expr	$CRIT1 \+ $CRIT2`
CTRL[1]=2
ERROR[1]="? NOT CORRECT"

# ?
CRIT1=`$GREP "IA * 1\.691422 * (0| )\.000000 * 1\.000000 * (\-0| \-)\.000000" $log | wc -l`
CRIT2=`$GREP "IB * 1\.691422 * 1\.000000 * (0| )\.000000 * (0| )\.000000" $log | wc -l`
CRIT3=`$GREP "IC * 2\.658560 * (0| )\.000000 * (0| )\.000000 * 1\.000000" $log | wc -l`
TEST[2]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3`
CTRL[2]=3
ERROR[2]="? NOT CORRECT"

# ?
CRIT1=`$GREP "Rotational constants" $log | wc -l`
CRIT2=`$GREP "\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-" $log | wc -l`
TEST[3]=`expr	$CRIT1 \+ $CRIT2`
CTRL[3]=2
ERROR[3]="? NOT CORRECT"

# ?
CRIT1=`$GREP "A * B * C" $log | wc -l`
TEST[4]=`expr	$CRIT1`
CTRL[4]=1
ERROR[4]="? NOT CORRECT"

# ?
CRIT1=`$GREP "298789\.4155 * 298789\.4155 * 190095\.0514 MHz" $log | wc -l`
CRIT2=`$GREP "9\.966542 * 9\.966542 * 6\.340888 cm\-1" $log | wc -l`
TEST[5]=`expr	$CRIT1 \+ $CRIT2`
CTRL[5]=2
ERROR[5]="? NOT CORRECT"

# ?
CRIT1=`$GREP "\@ * Nuclear repulsion energy \: * 11\.954105241814 Hartree" $log | wc -l`
TEST[6]=`expr	$CRIT1`
CTRL[6]=1
ERROR[6]="? NOT CORRECT"

# ?
CRIT1=`$GREP "Orbital exponents and contraction coefficients" $log | wc -l`
CRIT2=`$GREP "\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-" $log | wc -l`
TEST[7]=`expr	$CRIT1 \+ $CRIT2`
CTRL[7]=2
ERROR[7]="? NOT CORRECT"

# ?
CRIT1=`$GREP "N * 1s * 1 * 4173\.511000 * (0| )\.0018 * (0| )\.0000 * (0| )\.0000" $log | wc -l`
CRIT2=`$GREP "seg\. cont\. * 2 * 627\.457900 * (0| )\.0140 * (0| )\.0000 * (0| )\.0000" $log | wc -l`
CRIT3=`$GREP "3 * 142\.902100 * (0| )\.0686 * (0| )\.0000 * (0| )\.0000" $log | wc -l`
CRIT4=`$GREP "4 * 4(0| )\.234330 * (0| )\.2322 * (0| )\.0000 * (0| )\.0000" $log | wc -l`
CRIT5=`$GREP "5 * 12\.820210 * (0| )\.4691 * (0| )\.0000 * (0| )\.0000" $log | wc -l`
CRIT6=`$GREP "6 * 4\.390437 * (0| )\.3605 * (0| )\.0000 * (0| )\.0000" $log | wc -l`
CRIT7=`$GREP "7 * 11\.626358 * (0| )\.0000 * (\-0| \-)\.1150 * (0| )\.0000" $log | wc -l`
CRIT8=`$GREP "8 * 2\.716280 * (0| )\.0000 * (\-0| \-)\.1691 * (0| )\.0000" $log | wc -l`
CRIT9=`$GREP "9 * (0| )\.772218 * (0| )\.0000 * 1\.1459 * (0| )\.0000" $log | wc -l`
CRIT10=`$GREP "10 * (0| )\.212031 * (0| )\.0000 * (0| )\.0000 * 1\.0000" $log | wc -l`
TEST[8]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4 \+ $CRIT5 \+ $CRIT6 \+ \
		$CRIT7 \+ $CRIT8 \+ $CRIT9 \+ $CRIT10`
CTRL[8]=10
ERROR[8]="? NOT CORRECT"

# ?
CRIT1=`$GREP "N * 2px * 11 * 11\.626358 * (0| )\.0676 * (0| )\.0000" $log | wc -l`
CRIT2=`$GREP "seg\. cont\. * 12 * 2\.716280 * (0| )\.3239 * (0| )\.0000" $log | wc -l`
CRIT3=`$GREP "13 * (0| )\.772218 * (0| )\.7409 * (0| )\.0000" $log | wc -l`
CRIT4=`$GREP "14 * (0| )\.212031 * (0| )\.0000 * 1\.0000" $log | wc -l`
