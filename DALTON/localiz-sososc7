CRIT4=`$GREP "\@ * Number of orbital rotations * 50" $log | wc -l`
CRIT5=`$GREP "\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-\-" $log | wc -l`
CRIT6=`$GREP "\@ * Total number of variables * 51" $log | wc -l`
TEST[20]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4 \+ $CRIT5 \+ $CRIT6`
CTRL[20]=6
ERROR[20]="? NOT CORRECT"

# ?
CRIT1=`$GREP "Maximum number of Fock * iterations * 0" $log | wc -l`
CRIT2=`$GREP "Maximum number of DIIS * iterations * 60" $log | wc -l`
CRIT3=`$GREP "Maximum number of QC\-SCF iterations * 60" $log | wc -l`
